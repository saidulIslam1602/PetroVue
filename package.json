    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "cypress": "^14.5.4",
    "eslint-plugin-storybook": "^9.0.7",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "storybook": "^8.6.14",
    "ts-jest": "^29.4.4",
    "webpack": "^5.102.1"
